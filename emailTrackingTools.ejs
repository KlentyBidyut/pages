                var free_product = ['RocketBolt', 'OBTrack', 'MailTracker', 'GetNotify', 'Sparkpost'];
                var selected_plan = null;
                $("#free_tools").click(function(){
                    if((!selected_plan) || (selected_plan && selected_plan == 'paid')){
                        selected_plan = 'free';
                        hidePaidProducts();
                        showFreeProducts();
                        $('.kl_cta').hide();
                        $("#free_tools").addClass('bt_active');
                    } else {
                        selected_plan = null;
                        showPaidProducts();
                        $('.kl_cta').show();
                        $("#free_tools").removeClass('bt_active');
                    }
                    $("#paid_tools").removeClass('bt_active');
                });
                $("#paid_tools").click(function(){
                    if((!selected_plan) || (selected_plan && selected_plan == 'free')){
                        selected_plan = 'paid';
                        hideFreeProducts();
                        showPaidProducts();
                        $('.kl_cta').hide();
                        $("#paid_tools").addClass('bt_active');
                    } else {
                        selected_plan = null;
                        showFreeProducts();
                        $('.kl_cta').show();
                        $("#paid_tools").removeClass('bt_active');
                    }
                    $("#free_tools").removeClass('bt_active');
                });
                function hidePaidProducts(){
                    paid_product.forEach(function(product){
                        $('#'+product).hide();
                        $('#'+product+'_line').hide();
                    });
                };
                function showPaidProducts(){
                    paid_product.forEach(function(product){
                        $('#'+product).show();
                        $('#'+product+'_line').show();
                    });
                };
                function hideFreeProducts(){
                    free_product.forEach(function(product){
                        $('#'+product).hide();
                        $('#'+product+'_line').hide();
                    });
                };
                function showFreeProducts(){
                    free_product.forEach(function(product){
                        $('#'+product).show();
                        $('#'+product+'_line').show();
                    });
                };

            });
      </script>
      
